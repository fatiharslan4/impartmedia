{
	"items": {
		"$schema": "http://json-schema.org/draft-04/schema#",
		"$ref": "#/definitions/Hive"
	},
	"type": "array",
	"title": "Hives",
	"definitions": {
		"Hive": {
			"required": [
				"hiveId",
				"hiveName",
				"hiveDescription",
				"pinnedPostId",
				"tagComparisons"
			],
			"properties": {
				"hiveId": {
					"type": "integer"
				},
				"hiveName": {
					"type": "string"
				},
				"hiveDescription": {
					"type": "string"
				},
				"hiveDistributions": {
					"items": {
						"$schema": "http://json-schema.org/draft-04/schema#",
						"$ref": "#/definitions/HiveDistribution"
					},
					"type": "array"
				},
				"pinnedPostId": {
					"type": "integer"
				},
				"tagComparisons": {
					"items": {
						"$schema": "http://json-schema.org/draft-04/schema#",
						"$ref": "#/definitions/TagComparison"
					},
					"type": "array"
				}
			},
			"additionalProperties": false,
			"type": "object"
		},
		"HiveDistribution": {
			"required": [
				"displayText",
				"displayValue",
				"sortValue"
			],
			"properties": {
				"displayText": {
					"type": "string"
				},
				"displayValue": {
					"type": "string"
				},
				"sortValue": {
					"type": "integer"
				}
			},
			"additionalProperties": false,
			"type": "object"
		},
		"Percentile": {
			"required": [
				"percent",
				"highValue"
			],
			"properties": {
				"percent": {
					"type": "integer"
				},
				"highValue": {
					"type": "integer"
				},
				"displayValue": {
					"type": "string"
				},
				"fill": {
					"type": "boolean"
				}
			},
			"additionalProperties": false,
			"type": "object"
		},
		"TagComparison": {
			"required": [
				"tagId",
				"sortOrder",
				"percentile",
				"displayPercentile",
				"value",
				"displayScope",
				"percentiles"
			],
			"properties": {
				"tagId": {
					"type": "integer"
				},
				"sortOrder": {
					"type": "integer"
				},
				"percentile": {
					"type": "integer"
				},
				"displayPercentile": {
					"type": "string"
				},
				"value": {
					"type": "integer"
				},
				"displayValue": {
					"type": "string"
				},
				"displayScope": {
					"type": "string"
				},
				"displayDescription": {
					"type": "string"
				},
				"percentiles": {
					"items": {
						"$schema": "http://json-schema.org/draft-04/schema#",
						"$ref": "#/definitions/Percentile"
					},
					"type": "array"
				}
			},
			"additionalProperties": false,
			"type": "object"
		}
	}
}
{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"$ref": "#/definitions/PagedCommentsResponse",
	"title": "PagedCommentsResponse",
	"definitions": {
		"Comment": {
			"required": [
				"postId",
				"impartWealthId",
				"screenName",
				"content",
				"upVotes",
				"downVotes"
			],
			"properties": {
				"postId": {
					"type": "integer"
				},
				"commentId": {
					"type": "integer"
				},
				"commentDatetime": {
					"type": "string",
					"format": "date-time"
				},
				"impartWealthId": {
					"maxLength": 27,
					"minLength": 27,
					"type": "string"
				},
				"screenName": {
					"type": "string"
				},
				"content": {
					"$schema": "http://json-schema.org/draft-04/schema#",
					"$ref": "#/definitions/Content"
				},
				"edits": {
					"items": {
						"$schema": "http://json-schema.org/draft-04/schema#",
						"$ref": "#/definitions/Edit"
					},
					"type": "array"
				},
				"upVotes": {
					"type": "integer"
				},
				"downVotes": {
					"type": "integer"
				},
				"postCommentTrack": {
					"$schema": "http://json-schema.org/draft-04/schema#",
					"$ref": "#/definitions/PostCommentTrack"
				}
			},
			"additionalProperties": false,
			"type": "object"
		},
		"Content": {
			"required": [
				"markdown"
			],
			"properties": {
				"markdown": {
					"maxLength": 300000,
					"type": "string"
				}
			},
			"additionalProperties": false,
			"type": "object"
		},
		"Edit": {
			"required": [
				"impartWealthId",
				"screenName",
				"datetime",
				"deleted"
			],
			"properties": {
				"impartWealthId": {
					"maxLength": 27,
					"minLength": 27,
					"type": "string"
				},
				"screenName": {
					"type": "string"
				},
				"datetime": {
					"type": "string",
					"format": "date-time"
				},
				"notes": {
					"type": "string"
				},
				"deleted": {
					"type": "boolean"
				}
			},
			"additionalProperties": false,
			"type": "object"
		},
		"NextPage": {
			"required": [
				"offset"
			],
			"properties": {
				"offset": {
					"type": "integer"
				}
			},
			"additionalProperties": false,
			"type": "object"
		},
		"PagedCommentsResponse": {
			"required": [
				"comments",
				"nextPage"
			],
			"properties": {
				"comments": {
					"items": {
						"$schema": "http://json-schema.org/draft-04/schema#",
						"$ref": "#/definitions/Comment"
					},
					"type": "array"
				},
				"nextPage": {
					"$schema": "http://json-schema.org/draft-04/schema#",
					"$ref": "#/definitions/NextPage"
				}
			},
			"additionalProperties": false,
			"type": "object"
		},
		"PostCommentTrack": {
			"required": [
				"impartWealthId",
				"contentId",
				"upVoted",
				"downVoted"
			],
			"properties": {
				"impartWealthId": {
					"maxLength": 27,
					"minLength": 27,
					"type": "string"
				},
				"contentId": {
					"type": "integer"
				},
				"postId": {
					"type": "integer"
				},
				"upVoted": {
					"type": "boolean"
				},
				"downVoted": {
					"type": "boolean"
				},
				"votedDatetime": {
					"type": "string",
					"format": "date-time"
				}
			},
			"additionalProperties": false,
			"type": "object"
		}
	}
}
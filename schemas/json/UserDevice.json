					"type": "string"
				},
				"deviceId": {
					"type": "string"
				},
				"deviceToken": {
					"type": "string"
				},
				"appVersion": {
					"type": "string"
				},
				"deviceName": {
					"type": "string"
				},
				"deviceVersion": {
					"type": "string"
				},
				"createdAt": {
					"type": "string",

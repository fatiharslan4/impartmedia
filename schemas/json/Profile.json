{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"$ref": "#/definitions/Profile",
	"title": "Profile",
	"definitions": {
		"Address": {
			"properties": {
				"updatedDate": {
					"type": "string",
					"format": "date-time"
				},
				"address1": {
					"type": "string"
				},
				"address2": {
					"type": "string"
				},
				"city": {
					"type": "string"
				},
				"state": {
					"maxLength": 2,
					"minLength": 2,
					"type": "string"
				},
				"zip": {
					"type": "string"
				}
			},
			"additionalProperties": false,
			"type": "object"
		},
		"Attributes": {
			"properties": {
				"updatedDate": {
					"type": "string",
					"format": "date-time"
				},
				"name": {
					"type": "string"
				},
				"address": {
					"$schema": "http://json-schema.org/draft-04/schema#",
					"$ref": "#/definitions/Address"
				}
			},
			"additionalProperties": false,
			"type": "object"
		},
		"HiveMembership": {
			"required": [
				"hiveId"
			],
			"properties": {
				"hiveId": {
					"type": "integer"
				},
				"hiveName": {
					"type": "string"
				}
			},
			"additionalProperties": false,
			"type": "object"
		},
		"Profile": {
			"required": [
				"impartWealthId",
				"authenticationId",
				"email"
			],
			"properties": {
				"impartWealthId": {
					"maxLength": 27,
					"minLength": 27,
					"type": "string"
				},
				"authenticationId": {
					"type": "string"
				},
				"email": {
					"type": "string",
					"format": "email"
				},
				"screenName": {
					"maxLength": 35,
					"minLength": 4,
					"type": "string"
				},
				"admin": {
					"type": "boolean"
				},
				"attributes": {
					"$schema": "http://json-schema.org/draft-04/schema#",
					"$ref": "#/definitions/Attributes"
				},
				"createdDate": {
					"type": "string",
					"format": "date-time"
				},
				"updatedDate": {
					"type": "string",
					"format": "date-time"
				},
				"deviceToken": {
					"type": "string"
				},
				"hives": {
					"items": {
						"$schema": "http://json-schema.org/draft-04/schema#",
						"$ref": "#/definitions/HiveMembership"
					},
					"type": "array"
				},
				"isMember": {
					"type": "boolean"
				},
				"isBlocked": {
					"type": "boolean"
				},
				"devices": {
					"items": {
						"$schema": "http://json-schema.org/draft-04/schema#",
						"$ref": "#/definitions/UserDevice"
					},
					"type": "array"
				},
				"settings": {
					"$schema": "http://json-schema.org/draft-04/schema#",
					"$ref": "#/definitions/UserSettings"
				}
			},
			"additionalProperties": false,
			"type": "object"
		},
		"UserDevice": {
			"required": [
				"deviceId",
				"appVersion",
				"deviceName",
				"deviceVersion"
			],
			"properties": {
				"token": {
					"type": "string"
				},
				"impartWealthId": {
					"type": "string"
				},
				"deviceId": {
					"type": "string"
				},
				"deviceToken": {
					"type": "string"
				},
				"appVersion": {
					"type": "string"
				},
				"deviceName": {
					"type": "string"
				},
				"deviceVersion": {
					"type": "string"
				},
				"createdAt": {
					"type": "string",
					"format": "date-time"
				},
				"updatedAt": {
					"type": "string",
					"format": "date-time"
				}
			},
			"additionalProperties": false,
			"type": "object"
		},
		"UserSettings": {
			"required": [
				"notificationStatus"
			],
			"properties": {
				"notificationStatus": {
					"type": "boolean"
				}
			},
			"additionalProperties": false,
			"type": "object"
		}
	}
}